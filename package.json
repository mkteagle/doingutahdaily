{
  "name": "doingutahdaily-monorepo",
  "version": "1.0.0",
  "description": "doingutahdaily monorepo with public site and admin dashboard",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "dev": "pnpm --filter web dev",
    "dev:all": "pnpm -r dev",
    "build": "pnpm -r build",
    "start": "pnpm --filter web start",
    "lint": "pnpm -r lint",
    "db:migrate": "pnpm --filter db migrate dev",
    "db:push": "pnpm --filter db push",
    "db:seed": "pnpm --filter db seed"
  },
  "devDependencies": {
    "next": "^16.0.7"
  }
}
